<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" auto-import="true" assembly="Challengue" namespace="Challengue.Models">
	<class name="Profile" table="Profile" dynamic-update="true">
		<cache usage="read-write"/>
		<id name="Id" column="Id" type="long">
			<generator class="native"/>
		</id>
		<property name="Name" />
		<property name="Age" />
		<property name="City" />
	</class>
</hibernate-mapping>